{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["standard", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "semi": 0,
        "quotes": 0,
        "react/react-in-jsx-scope": 0,
        "react/prop-types": 0,
        "strict-boolean-expressions": 0,
        "no-confusing-void-expression": 0,
        "explicit-function-return-type": 0,
        "indent": "off"
    }
}
